<%= form_for(@post) do |f| %>

<% @post.user_id = session[:user_id] %>
<% @post.note = 0.0 %>

  <div class="field">
    <%= f.hidden_field :user_id %><br />
  </div>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <br />
  <div class="field">
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </div>
  <div class="field">
    <%= f.hidden_field :note %><br />
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
  <br />
<% end %>
